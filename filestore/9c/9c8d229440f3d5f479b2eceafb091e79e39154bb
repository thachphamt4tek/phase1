var constant=x=>()=>x;function linear(a,d){return function(t){return a+t*d;};}
function exponential(a,b,y){return a=Math.pow(a,y),b=Math.pow(b,y)-a,y=1/y,function(t){return Math.pow(a+t*b,y);};}
