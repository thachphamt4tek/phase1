class CarouselPanel extends Component{static template="o-spreadsheet-CarouselPanel";static props={onCloseSidePanel:Function,figureId:String};static components={Section,TextInput,TextStyler,CogWheelMenu};DEFAULT_CAROUSEL_TITLE_STYLE=DEFAULT_CAROUSEL_TITLE_STYLE;dragAndDrop=useDragAndDropListItems();previewListRef=useRef("previewList");setup(){let lastCarouselItems=[...this.carouselItems];onWillUpdateProps(()=>{if(!deepEquals(this.carouselItems,lastCarouselItems)){this.dragAndDrop.cancel();}
