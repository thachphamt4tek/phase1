class ImageFigure extends Component{static template="o-spreadsheet-ImageFigure";static props={figureUI:Object,editFigureStyle:{type:Function,optional:true},openContextMenu:{type:Function,optional:true},};static components={};get figureId(){return this.props.figureUI.id;}
