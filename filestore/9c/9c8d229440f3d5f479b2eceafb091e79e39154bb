return result.balance;}
getAccountTagData(accountTagIds,dateRange,offset,companyId,includeUnposted){dateRange=deepCopy(dateRange);dateRange.year+=offset;if(dateRange.year<1900){throw new EvaluationError(_t("%s is not a valid year.",dateRange.year));}
