throw new Error(`${irMenuUrl} is not a valid menu xml link`);}
__exports.buildIrMenuXmlLink=buildIrMenuXmlLink;function buildIrMenuXmlLink(menuXmlId){return`${IR_MENU_XML_ID_PREFIX}${menuXmlId}`;}
return __exports;});;

