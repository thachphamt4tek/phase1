`;class FigureComponent extends Component{static template="o-spreadsheet-FigureComponent";static props={figureUI:Object,style:{type:String,optional:true},class:{type:String,optional:true},onMouseDown:{type:Function,optional:true},onClickAnchor:{type:Function,optional:true},};static components={MenuPopover};static defaultProps={onMouseDown:()=>{},onClickAnchor:()=>{},};menuState=useState({isOpen:false,anchorRect:null,menuItems:[]});figureRef=useRef("figure");figureWrapperRef=useRef("figureWrapper");menuButtonRef=useRef("menuButton");borderWidth;get isSelected(){return this.env.model.getters.getSelectedFigureId()===this.props.figureUI.id;}
get figureRegistry(){return figureRegistry;}
getBorderWidth(){if(this.env.isDashboard())
