class ComboChartDesignPanel extends GenericZoomableChartDesignPanel{static template="o-spreadsheet-ComboChartDesignPanel";static components={...GenericZoomableChartDesignPanel.components,ChartShowDataMarkers,RadioSelection,};seriesTypeChoices=[{value:"bar",label:_t("Bar")},{value:"line",label:_t("Line")},];updateDataSeriesType(index,type){const dataSets=[...this.props.definition.dataSets];if(!dataSets?.[index]){return;}
dataSets[index]={...dataSets[index],type,};this.props.updateChart(this.props.chartId,{dataSets});}
