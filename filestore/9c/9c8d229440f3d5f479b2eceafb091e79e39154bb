class ChartWithAxisDesignPanel extends Component{static template="o-spreadsheet-ChartWithAxisDesignPanel";static components={GeneralDesignEditor,SidePanelCollapsible,Section,AxisDesignEditor,SeriesWithAxisDesignEditor,ChartLegend,ChartShowValues,ChartHumanizeNumbers,};static props={chartId:String,definition:Object,canUpdateChart:Function,updateChart:Function,};get axesList(){const{useLeftAxis,useRightAxis}=getDefinedAxis(this.props.definition);const axes=[{id:"x",name:_t("Horizontal axis")}];if(useLeftAxis){axes.push({id:"y",name:useRightAxis?_t("Left axis"):_t("Vertical axis")});}
if(useRightAxis){axes.push({id:"y1",name:useLeftAxis?_t("Right axis"):_t("Vertical axis")});}
return axes;}}
